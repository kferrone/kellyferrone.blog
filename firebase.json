{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
          "source": "/contact-me",
          "function": "sendgridEmail"
      },
      {
          "source": "/sitemap.xml",
          "function": "siteMap"
	  },
	  {
		  "source": "/rss.xml",
		  "function": "rssFeed"
	  },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
